/* pages/index/index.wxss */
.card {
  display: flex;
  flex-direction: row;
  height: 200rpx;
  width: 80vw;
  border: 1px solid gray;
  border-radius: 5px;
  margin: 5%;
}
.cardtext{
  display: flex;
  flex-direction: column;
}
image{
  width:30%;
  object-fit: cover;
  height: auto;
}
.container {
  display: flex;
  flex-direction: column;
  width: 100vw;
  align-items:center;
}